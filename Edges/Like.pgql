# pho-graphql-v1

type Like implements SubscribeEdge 
@nodes(head:"Status", tail:"User")
@properties(binding: false, persistent: true, consumer: true, multiplicable: false)
@labels(
	headSingular:"like", 
	headPlural: "likes", 
	tailSingular: "liker", 
	tailPlural: "likers",
	feedShort: "%tail.id% liked %head.id%",
	feedLong: "%tail.id% liked %head.id%"
)
{
    id: ID!
}